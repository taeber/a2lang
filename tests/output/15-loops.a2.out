	ORG $2000
COUT	EQU $FDED
DOS	EQU $3D0
HOME	EQU $FC58
INIT	EQU $FB2F
RDKEY	EQU $FD0C
	JSR INIT
	JSR HOME
CR	EQU $8D
* COPYBB key #CR
	LDA #CR
	STA key
* BRA A2_1
A2_0	CLV
	BVC A2_1
* IFNE #$01 #$00
*   A2_3 A2_4
A2_1	LDA #$01
	BNE A2_3
	JMP A2_4
A2_3	JSR RDKEY
* COPYBB key @A
	STA key
* IFEQ key #"A"
*   A2_6 A2_7
A2_5	LDA key
	CMP #"A"
	BEQ A2_6
	JMP A2_7
* REPEAT
A2_6	JMP A2_1
A2_7	JSR COUT
* IFEQ key #"Q"
*   A2_9 A2_10
A2_8	LDA key
	CMP #"Q"
	BEQ A2_9
	JMP A2_10
* STOP
A2_9	JMP A2_4
A2_10	JMP A2_1
* STOP
A2_4	JMP A2_2
	JMP A2_0
A2_2	JSR DOS
key	HEX 00
