let NUMPEEPS = 10

var i: int
i := 0
while i < NUMPEEPS {
    i += 1
}
while i > 0 {
    i -= NUMPEEPS
}

let (
bool = :byte,
false = 0,
true = 1,
)

var [
    done: bool,
    working: bool,
]
done := false
; In C, this would be  working = !done
working != done
done := true

var flags: byte
flags := %00000000
flags |= %11111111  ; ORA
flags &= %10101010  ; AND
flags ~= %10101010  ; EOR

