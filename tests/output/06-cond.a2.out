FALSE	EQU $00
TRUE	EQU $01
* COPYBB Check.is #FALSE
Check	LDA #FALSE
	STA Check.is
* IFEQ Check.val #$00
*   Check._1 Check._2
Check._0	LDA Check.val
	BEQ Check._1
	JMP Check._2
* COPYBB Check.is #TRUE
Check._1	LDA #TRUE
	STA Check.is
	RTS
* IFNE Check.val #$00
*   Check._3 Check._4
Check._2	LDA Check.val
	BNE Check._3
	JMP Check._4
Check._3	RTS
* IFGE #$00 Check.val
*   Check._5 Check._6
Check._4	LDA #$00
	CMP Check.val
	BCS Check._5
	JMP Check._6
* IFLT Check.val #$00
*   Check._7 Check._8
Check._5	LDA Check.val
	BCC Check._7
	JMP Check._8
Check._7	RTS
* COPYBB Check.is #TRUE
Check._8	LDA #TRUE
	STA Check.is
	RTS
* IFGE #$00 Check.val
*   Check._9 Check._10
Check._6	LDA #$00
	CMP Check.val
	BCS Check._9
	JMP Check._10
* IFLT Check.val #$00
*   Check._11 Check._12
Check._9	LDA Check.val
	BCC Check._11
	JMP Check._12
Check._11	RTS
* COPYBB Check.is #TRUE
Check._12	LDA #TRUE
	STA Check.is
	RTS
Check._10	RTS
Check.val	HEX 00
Check.is	HEX 00
