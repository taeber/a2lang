*
	ORG $800
	JSR main
	JMP EXIT
EXIT	EQU $3D0
INIT	EQU $FB2F
HOME	EQU $FC58
COUT	EQU $FDED
CROUT	EQU $FD8E
DST	EQU $06
SRC	EQU $08
swap1.dst	EQU DST
swap1.src	EQU SRC
* COPYWW DST SRC
swap1	LDA SRC
	LDX SRC+1
	STA DST
	STX DST+1
swap1._end	RTS
main	JSR INIT
	JSR HOME
	LDA #<A2_0
	LDX #>A2_0
	STX swap1.dst+1
	STA swap1.dst
	LDA #<A2_1
	LDX #>A2_1
	STX swap1.src+1
	STA swap1.src
	JSR swap1
* COPYWW DST SRC
	LDA SRC
	LDX SRC+1
	STA DST
	STX DST+1
main.world	EQU DST
* COPYBB @A (main.world),#$00
	LDY #$00
	LDA (main.world),Y
	JSR COUT
* COPYBB @A (main.world),#$01
	LDY #$01
	LDA (main.world),Y
	JSR COUT
* COPYBB @A (main.world),#$02
	LDY #$02
	LDA (main.world),Y
	JSR COUT
* COPYBB @A (main.world),#$03
	LDY #$03
	LDA (main.world),Y
	JSR COUT
* COPYBB @A (main.world),#$04
	LDY #$04
	LDA (main.world),Y
	JSR COUT
	JSR CROUT
main._end	RTS
A2_0	ASC "HELLO"
	HEX 00
A2_1	ASC "WORLD"
	HEX 00
